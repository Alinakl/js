function rand(min, max) {
  return Math.floor(Math.random() * (max - min)) + min;
}

var arr = [];
var ARR_SIZE = 10;

for (var i = 0; i < ARR_SIZE; i++) {
  arr[i] = [];
  for (var j = 0; j < ARR_SIZE; j++) {
    arr[i][j] = rand(0, 99);
  }
}

console.table(arr);

var flatten = [];

for (var i = 0; i < arr.length; i++) {
  flatten.push(arr[i][0]);
  flatten.push(arr[i][arr[i].length - 1]);
}

console.table(flatten);
