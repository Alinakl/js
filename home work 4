function rand(min, max) {
     return Math.floor(Math.random() * (max - min)) + min;
}
var arr = [];

/*
		var arr = [ [], [], [] ];
*/

var ARR_SIZE = 10;
for (var i = 0; i < ARR_SIZE; i++) {
	arr[i] = [];
	for (var j = 0; j < ARR_SIZE; j++) {
		// arr[i].push( rand(0, 199) );
		arr[i][j] = rand(0, 99);
	}
}
console.table(arr);

